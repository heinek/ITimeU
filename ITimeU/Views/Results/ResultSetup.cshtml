@{
    ViewBag.Title = "ResultSetup";
}
<script src="@Url.Content("~/Scripts/jquery-1.4.4.js")" type="text/javascript"></script>
<script src="@Url.Content("/Scripts/jquery.validate.min.js")" type="text/javascript"></script>
<script src="@Url.Content("/Scripts/jquery.validate.unobtrusive.min.js")" type="text/javascript"></script>
<script src="@Url.Content("~/Scripts/Tools.js")" type="text/javascript"></script>
<script type="text/javascript">
    $(document).ready(function () {
        var imgApprove = $("#imgApprove");
        var imgResults = $("#imgResults");

        imgApprove.click(function () {
            var url = "/Results/";
            document.location = url;
        });

        imgResults.click(function () {
            var url = "/Results/SelectEvent/";
            document.location = url;
        });

        $("img").hover(
            function () {
                this.src = this.src.replace("_off", "_on");
            },
            function () {
                this.src = this.src.replace("_on", "_off");
            }
        );
    });
</script>
<div class="topmenubar">
    <center>
        | @Html.ActionLink("HJEM", "Index", "Home", null, new { @class = "topmenulink" })
        |
    </center>
</div>
<div class="container">
    <div class="background">
        <div class="row">
            <div class="menubg2">
                <img src="../../Content/images/godkjenn_off.png" id="imgApprove" alt="Godkjenn resultater"
                    class="logo" /><img id="imgResults" src="../../Content/images/resultatliste_off.png"
                        alt="Resultatliste" class="logo" />
            </div>
        </div>
    </div>
</div>
