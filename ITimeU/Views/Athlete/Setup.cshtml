@{
    ViewBag.Title = "Setup";
}
<script src="@Url.Content("~/Scripts/jquery-1.4.4.js")" type="text/javascript"></script>
<script src="@Url.Content("/Scripts/jquery.validate.min.js")" type="text/javascript"></script>
<script src="@Url.Content("/Scripts/jquery.validate.unobtrusive.min.js")" type="text/javascript"></script>
<script src="@Url.Content("~/Scripts/Tools.js")" type="text/javascript"></script>
<script type="text/javascript">
    $(document).ready(function () {
        var imgAthletes = $("#imgAthletes");
        var imgClasses = $("#imgClasses");

        imgAthletes.click(function () {
            var url = "/Athlete/";
            document.location = url;
        });

        imgClasses.click(function () {
            var url = "/Class/";
            document.location = url;
        });

        $("img").hover(
            function () {
                this.src = this.src.replace("_off", "_on");
            },
            function () {
                this.src = this.src.replace("_on", "_off");
            }
        );
    });
</script>
<div class="topmenubar">
    <center>
        | @Html.ActionLink("HJEM", "Index", "Home", null, new { @class = "topmenulink" })
        | @Html.ActionLink("ADMIN", "Index", "Admin", null, new { @class = "topmenulink" })
        |
    </center>
</div>
<div class="container">
    <div class="background">
        <div class="row">
            <div class="menubg2">
                <center>
                    <img src="../../Content/images/loperlist_off.png" id="imgAthletes" alt="Løpere" class="logo" /><img
                        id="imgClasses" src="../../Content/images/classes_off.png" alt="Klasser"
                        class="logo" />
                </center>
            </div>
        </div>
    </div>
</div>
